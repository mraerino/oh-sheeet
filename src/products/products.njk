---
layout: layouts/products.njk
title: Products
templateClass: tmpl-products
eleventyNavigation:
  key: Products
  order: 2
---

<div class="wrapper">
  <ul>
  {%- for category, list in products -%}
  <li>
    <h2>
    <a href="/products/{{category | kebabify}}">{{category}}</a>
    </h2>
    <ul class="product-category--items">
    {%- for product in list -%}
    <li>
      <img src="{{ product.img }}"  alt="blerb" alt="">
      <p>{{ product.productName }}</p>
    </li>
    {%- endfor %}
    </ul>
  </li>
  {%- endfor -%}
  </ul>
</div>